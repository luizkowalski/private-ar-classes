<turbo-frame id="upvote--post-<%= post.id %>">
  <%= button_to upvote_subreddit_post_path(subreddit_id: subreddit_id, id: post.id), class: 'level-item button is-info is-light is-small' do %>
    <span>👍🏻</span>
    <span data-vote-target="upvotes">&nbsp;<%= post.votes %></span>
  <% end %>
</turbo-frame>